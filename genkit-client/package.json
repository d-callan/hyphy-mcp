{
  "name": "hyphy-genkit-client",
  "version": "0.1.0",
  "description": "Genkit client for HyPhy MCP server",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev:backend": "ts-node src/index.ts",
    "dev:server": "ts-node src/server.ts",
    "dev:ui": "cd ../genkit-client-ui && npm run dev",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:ui\"",
    "lint": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "hyphy",
    "mcp",
    "genkit",
    "client"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@genkit-ai/compat-oai": "^1.15.5",
    "@genkit-ai/googleai": "^1.15.5",
    "dotenv": "^17.2.1",
    "genkit": "^1.15.5",
    "genkitx-anthropic": "^0.23.1",
    "genkitx-mcp": "^1.14.1",
    "genkitx-ollama": "^1.15.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^20.11.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "svelte": "^5.37.3",
    "svelte-loader": "^3.2.4",
    "svelte-preprocess": "^6.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
